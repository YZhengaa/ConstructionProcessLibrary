expp:HumidityRuleShape rdf:type sh:NodeShape;
 rdf:type rdfs:Class ;
 rdfs:subClassof rdfs:Resource;
 sh:targetClass dicp:Activity;
 sh:rule [
        a sh:SPARQLRule ;
        sh:prefixes expp: ;
        sh:prefixes sosa: ;
        sh:prefixes dicp: ;
        sh:prefixes bot: ;
        sh:prefixes dice: ;
        sh:prefixes rdfs: ;
        sh:message "Painting is able to conduct, RH satisfy." ;
        sh:construct """
            CONSTRUCT {
                $this expp:monitoredby ?sensor.
            }
            WHERE {
                $this dicp:hasLocation ?location .
                ?location a bot:Space .
                ?location sosa:hosts ?sensor .
            }
            """
    
    ]
.
